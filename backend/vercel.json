{
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/api/index.py"
    }
  ],
  "functions": {
    "backend/api/index.py": {
      "includeFiles": "backend/**"
    }
  }
}